<%- include('../partials/header') %>
    
    
  <div class="container">
    <form class="bigForm" id="patientForm" action="/patients/new" method="POST">
      <div>
        <label for="mrn">MRN:</label>
        <input type="text" id="mrn" name="patient[mrn]">
      </div>
      <div>
        <label for="first_name">First Name:</label>
        <input type="text" id="first_name" name="patient[first_name]">
      </div>
      <div>
        <label for="last_name">Last Name:</label>
        <input type="text" id="last_name" name="patient[last_name]">
      </div>
     
      <button class="btn btn-primary" id="submitpatient">Submit</button>
    </form>  
    
    <form action="/patients/">
      <input type="submit" class="btn btn-secondary" value="Cancel" />
    </form>
    
  </div>

  

